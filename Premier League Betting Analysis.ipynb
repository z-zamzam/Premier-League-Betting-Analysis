   "source": [
    "import pandas as pd\n",
    "from sklearn.preprocessing import LabelEncoder\n",
    "from sklearn.metrics import accuracy_score, precision_score, recall_score, f1_score\n",
    "from tensorflow.keras.models import Sequential\n",
    "from tensorflow.keras.layers import Dense\n",
    "\n",
    "df = pd.read_csv('EnglandLeagues2.csv')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 67,
   "id": "b81934a6",
   "metadata": {},
   "outputs": [],
   "source": [
    "df = df[[c for c in df if c not in ['FTR']] + ['FTR']]\n",
    "for i in range(0, len(df['FTR'])):\n",
    "    if df['FTR'][i] == 'H':\n",
    "        df.loc[i, 'FTRB'] = 1\n",
    "    elif df['FTR'][i] == 'D':\n",
    "        df.loc[i, 'FTRB'] = 0.5\n",
    "    else:\n",
    "        df.loc[i, 'FTRB'] = 0"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 68,
   "id": "3b1faa49",
   "metadata": {},
   "outputs": [],
   "source": [
    "df = df.drop(['FTR', 'Date', 'Div', 'FTHG', 'FTAG', 'HTHG', 'HTAG', 'HTR', 'Time'], axis=1)\n",
    "df[['HomeTeam', 'AwayTeam']] = df[['HomeTeam', 'AwayTeam']].apply(LabelEncoder().fit_transform)\n",
    "df.dropna(inplace=True)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 69,
   "id": "a158b884",
   "metadata": {},
   "outputs": [],
   "source": [
    "df_1 = df.iloc[:3000,:]\n",
    "df_2 = df.iloc[3000:,:].reset_index().drop('index', axis=1)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 70,
   "id": "24277bfd",
   "metadata": {},
   "outputs": [],
   "source": [
    "X = df_1.drop('FTRB', axis=1)\n",
    "Y = df_1.drop(['HomeTeam', 'AwayTeam', 'B365H', 'B365D', 'B365A', 'BWH', 'BWD', 'BWA',\n",
    "       'IWH', 'IWD', 'IWA', 'PSH', 'PSD', 'PSA', 'WHH', 'WHD', 'WHA', 'VCH',\n",
    "       'VCD', 'VCA', 'MaxH', 'MaxD', 'MaxA', 'AvgH', 'AvgD', 'AvgA', 'B365>2.5', 'B365<2.5', 'P>2.5', 'P<2.5', 'Max>2.5', 'Max<2.5',\n",
    "       'Avg>2.5', 'Avg<2.5'], axis=1)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 71,
   "id": "e48b9661",
   "metadata": {},
   "outputs": [],
   "source": [
    "mean = X.mean(axis=0)\n",
    "X -= mean\n",
    "std = X.std(axis=0)\n",
    "X /= std"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 72,
   "id": "6d3b241f",
   "metadata": {},
   "outputs": [],
   "source": [
    "model = Sequential()\n",
    "model.add(Dense(8, input_dim = len(X.columns), activation=\"relu\"))\n",
    "model.add(Dense(4, activation=\"relu\"))\n",
    "model.add(Dense(1, activation=\"sigmoid\"))"
   ]
